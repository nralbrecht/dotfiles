[disc_root]
command=df | grep "/$" | awk '{ print "root " $5 }'
interval=60

[cpu_usage2]
command=/home/nralbrecht/.config/i3blocks/cpu_usage/cpu_usage -d 0
label=CPU
interval=5

[wifi]
command=INTERFACE=wlp1s0; /home/nralbrecht/.config/i3blocks/wifi/wifi
label=wifi:
interval=30

[battery]
command=/home/nralbrecht/.config/i3blocks/battery/battery
interval=30
label=bat:

[calendar]
interval=5
command=/home/nralbrecht/.config/i3blocks/calendar/calendar
